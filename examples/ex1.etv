contract SimpleStorage {
    uint(5) storedData;
    
    function set(uint(5) a) {
        storedData = a;
    }
    
    function get() returns (uint(5)) {
        return storedData;
    }
}

uint(5) x;
uint(5) y;

x = 2;


set.sendTransaction(x, {from: "0xabcde", value: 0});

y = get.call();

